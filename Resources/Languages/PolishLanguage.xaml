<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GymBro.Resources.Languages.PolishLanguage">
    <!-- Settings Header-->
    <x:String x:Key="Account">Konto</x:String>
    <x:String x:Key="Theme">Motyw</x:String>
    <x:String x:Key="Language">Język</x:String>
    <!--Theme labels-->
    <x:String x:Key="Dark">Ciemny</x:String>
    <x:String x:Key="Light">Jasny</x:String>
    <!--Languages labels-->
    <x:String x:Key="English">Angielski</x:String>
    <x:String x:Key="Russian">Rosyjski</x:String>
    <x:String x:Key="Polish">Polski</x:String>

    <x:String x:Key="ErrorMessage">Błąd! Spróbuj ponownie załadować stronę.</x:String>
    <x:String x:Key="Reload">Przeładuj</x:String>

    <!--Week day names-->
    <x:Array Type="{x:Type x:String}" x:Key="WeekNames">
        <x:String>Pon</x:String>
        <x:String>Wto</x:String>
        <x:String>Śro</x:String>
        <x:String>Czw</x:String>
        <x:String>Pią</x:String>
        <x:String>Sob</x:String>
        <x:String>Nie</x:String>
    </x:Array>

</ResourceDictionary>